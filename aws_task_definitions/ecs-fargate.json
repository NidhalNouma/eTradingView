{
    "family": "isalgo",
    "containerDefinitions": [
      {
        "name": "django-isalgo-live",
        "image": "358776226602.dkr.ecr.us-east-1.amazonaws.com/isalgo",
        "cpu": 256,
        "memory": 512,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8000,
            "hostPort": 8000
          }
        ],
        "environment": [
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/isalgo",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      } , {
          "name": "celery-worker",
          "image": "358776226602.dkr.ecr.us-east-1.amazonaws.com/isalgo",
          "cpu": 256,
          "memory": 512,
          "essential": true,
          "command": [
              "celery", "-A", "etradingview", "worker", "-l", "info"
          ],
          "environment": [
          ]
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "executionRoleArn": "arn:aws:iam::358776226602:role/ecsTaskExecutionRole"
  }
  